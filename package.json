{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:debug": "nuxi dev --inspect",
    "clean:all": "pnpm -r clean && rimraf .nuxt .output node_modules && nuxi cleanup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "eslint-config-view": "pnpx eslint-flat-config-viewer",
    "kill-port": "pnpx kill-port 3000",
    "kill-node": "bun x  taskkill /F /IM node.exe || exit 0",
    "nuxt:upgrade": "pnpx nuxi@latest upgrade --force",
    "typesync": "typesync",
    "db:push": "drizzle-kit push --config drizzle.config.ts",
    "db:generate": "dotenv -e .env drizzle-kit generate",
    "db:introspect": "dotenv -e .env drizzle-kit introspect",
    "db:studio": " drizzle-kit studio --host 127.0.0.1 --port 3333",
    "db:seed": "dotenv -e .env tsx server/database/seed.ts",
    "db:force-reset": "dotenv -e .env tsx server/database/force-reset-db.ts",
    "db:format": "dotenv -e .env tsx pnpm run db:force-reset && pnpm run db:seed"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@nuxt/devtools-kit": "^1.7.0",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.30.1",
    "drizzle-orm": "^0.38.3",
    "nuxi": "^3.20.0",
    "nuxt": "^3.15.1",
    "vue": "latest",
    "vue-router": "latest"
  },
  "packageManager": "pnpm@9.15.3"
}
